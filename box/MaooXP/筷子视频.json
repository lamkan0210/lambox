{

    "ua": "",
    // 首页地址 用于获取 分类和首页推荐
    "homeUrl": "https://www.kuaizi.cc/",
    "cateNode":"//li[@class='nav-menu-item']/a"
        // 分类名
	"cateName": "/text()",
    // 分类id
	"cateId": "/@href",
    // 首页推荐视频的节点
    "homeVodNode": "//div[@class='module-item-pic']",
    // 首页推荐视频的名称
    "homeVodName": "/a/@title",
    // 二次处理正则
    "homeVodNameR": "",
    // 首页推荐视频的id
    "homeVodId": "/a/@href",
    // 二次处理正则
    "homeVodIdR": "",
    // 首页推荐视频的图片
    "homeVodImg": "/img/@data-src",

    // 详情页地址 用于获取详情页信息 及 播放列表和地址
    "dtUrl": "https://www.kuaizi.cc/{vid}",
    // 详情节点
    "dtNode": "//body",
    // 视频名
    "dtName": "//div[@class='module-item-pic']/a/@title",
    // 视频图片
    "dtImg": "//div[@class='module-item-pic']/img/@data-src",
    // 播放源节点
    "dtFromNode": "//div[@class='module-tab-content']",
    // 播放源名称 多个重名播放源
    "dtFromName": "//div[contains(@class,'module-tab-item tab-item')]",
    // 播放列表节点
    "dtUrlNode": "//div[@class='scroll-content']",
    // 播放地址节点
    "dtUrlSubNode": "/a",
    // 播放地址
    "dtUrlId": "/a/@href",
    // 剧集名称
    "dtUrlName": "/text()",
    // 二次处理正则
    "dtUrlNameR": "",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
    "playUrl": "https://www.kuaizi.cc/{playUrl}",
    // 解析webview的user-agent
    "playUa": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/111.0.0.0 Safari/537.36",
    // 搜索地址
    "searchUrl": "https://www.kuaizi.cc/vodsearch/-------------/?wd={wd}",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
    "scVodNode": "//div[@class='module-item-pic']",
    "scVodName": "/a/@title",
    "scVodId": "/a/@href",
    "scVodImg": "/img/@data-src",
}
